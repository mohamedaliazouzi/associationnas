{"version": 3,
  "builds": [
    {
      "src": "associationnas/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }},
      {
      "src": "build.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "staticfiles"
      }
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    {
      "src": "/api/(.*)",
      "dest": "associationnas/wsgi.py"
    },
    {
      "src": "/(.*)",
      "dest": "associationnas/wsgi.py"
    }
  ]
}
